apiVersion: v1
kind: Pod
metadata:
  name: superset
spec:
  containers:
  - name: superset
    image: amancevice/superset
    ports:
    - containerPort: 80


——

apiVersion: v1
kind: Service
metadata:
  name: superset
  labels:
    run: superset
spec:
  ports:
  - port: 8080
    protocol: TCP
  selector:
    run: superset
